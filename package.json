    "build:electron": "pnpm build && electron .",
    "dist": "pnpm build && electron-builder"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "clsx": "^2.1.1",
    "next": "14.2.11",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.79.1"
  },
  "devDependencies": {
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^9.0.1",
    "electron": "^32.1.1",
    "electron-is-dev": "^3.0.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.6.2",
    "wait-on": "^8.0.1"
  }
}
