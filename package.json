{
  "name": "resoucepack-creator",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:next\" \"pnpm dev:electron\"",
    "dev:next": "next dev",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
